{%- liquid
  assign section_id = section.id
  assign top_heading = section.settings.top_heading
  assign top_heading_size = section.settings.top_heading_size | default: 4.8
  assign top_subheading = section.settings.top_subheading
  assign top_button_text = section.settings.top_button_text
  assign top_button_url = section.settings.top_button_url
  assign top_button_text_2 = section.settings.top_button_text_2
  assign top_button_url_2 = section.settings.top_button_url_2
  assign top_bg_type = section.settings.top_bg_type | default: 'image'
  assign top_bg_image = section.settings.top_bg_image
  assign top_bg_gradient = section.settings.top_bg_gradient
  assign top_min_height = section.settings.top_min_height | default: 60
  assign top_logo = section.settings.top_logo
  assign top_logo_width = section.settings.top_logo_width | default: 120
  assign top_logo_width_2x = top_logo_width | times: 2
  assign top_center_image = section.settings.top_center_image
  assign top_center_image_width = section.settings.top_center_image_width | default: 280
  assign top_center_image_width_2x = top_center_image_width | times: 2
  assign top_center_image_position = section.settings.top_center_image_position | default: 'center'
  assign top_center_image_y_offset = section.settings.top_center_image_y_offset | default: 0
  assign bottom_bg_color = section.settings.bottom_bg_color
  assign bottom_padding = section.settings.bottom_padding | default: 4
  assign features_layout = section.settings.features_layout | default: '3-columns'
  assign features_container_width = section.settings.features_container_width
  assign section_width = section.settings.section_width
  assign section_radius = section.settings.section_radius | default: 24
  assign button_location = section.settings.button_location | default: 'center'
  assign button_radius = section.settings.button_radius | default: 0.8
  assign button_primary_bg = section.settings.button_primary_bg
  assign button_primary_text = section.settings.button_primary_text
  assign button_primary_border = section.settings.button_primary_border
  assign button_secondary_bg = section.settings.button_secondary_bg
  assign button_secondary_text = section.settings.button_secondary_text
  assign button_secondary_border = section.settings.button_secondary_border
  assign feature_blocks = section.blocks | where: 'type', 'feature'
-%}

<section
  id="SplitHeroFeatures-{{ section_id }}"
  class="split-section split-section--{{ section_id }}"
  style="--split-section-radius: {{ section_radius }}px;--split-bottom-padding: {{ bottom_padding }}rem;--button-radius: {{ button_radius }}rem;--top-heading-size: {{ top_heading_size }}rem;{% if section_width != blank and section_width > 0 %} --split-section-width: {{ section_width }}px;{% endif %}{% if features_container_width != blank and features_container_width > 0 %} --features-container-width: {{ features_container_width }}px;{% endif %}{% if bottom_bg_color != blank %} --split-bottom-bg: {{ bottom_bg_color }};{% endif %}{% if button_primary_bg != blank %} --button-primary-bg: {{ button_primary_bg }};{% endif %}{% if button_primary_text != blank %} --button-primary-text: {{ button_primary_text }};{% endif %}{% if button_primary_border != blank %} --button-primary-border: {{ button_primary_border }};{% endif %}{% if button_secondary_bg != blank %} --button-secondary-bg: {{ button_secondary_bg }};{% endif %}{% if button_secondary_text != blank %} --button-secondary-text: {{ button_secondary_text }};{% endif %}{% if button_secondary_border != blank %} --button-secondary-border: {{ button_secondary_border }};{% endif %}{% if top_bg_type == 'gradient' and top_bg_gradient != blank %} --split-top-bg: {{ top_bg_gradient }};{% elsif top_bg_type == 'image' and top_bg_image != blank %} --split-top-bg-image: url({{ top_bg_image | image_url: width: 1600 }});{% endif %}"
>
  <div class="split-top {% if top_bg_type == 'gradient' and top_bg_gradient != blank %}split-top--gradient{% endif %}" {% if top_bg_type == 'gradient' and top_bg_gradient != blank %}style="background: {{ top_bg_gradient }}; background-image: none;"{% elsif top_bg_type == 'image' and top_bg_image != blank %}style="background-image: url({{ top_bg_image | image_url: width: 1600 }});"{% endif %}>
    {%- if top_logo != blank -%}
      <div class="split-top-logo" style="--top-logo-width: {{ top_logo_width }}px;">
        <img
          src="{{ top_logo | image_url: width: top_logo_width_2x }}"
          srcset="{{ top_logo | image_url: width: top_logo_width }} 1x, {{ top_logo | image_url: width: top_logo_width_2x }} 2x"
          alt="{{ top_logo.alt | default: 'Logo' | escape }}"
          loading="lazy"
          width="{{ top_logo_width }}"
          height="{{ top_logo.height | times: top_logo_width | divided_by: top_logo.width }}"
        >
      </div>
    {%- endif -%}
    {%- if top_center_image != blank -%}
      <div class="split-top-center-image split-top-center-image--{{ top_center_image_position }}" style="--top-center-image-width: {{ top_center_image_width }}px;--top-center-image-y-offset: {{ top_center_image_y_offset }}px;">
        <img
          src="{{ top_center_image | image_url: width: top_center_image_width_2x }}"
          srcset="{{ top_center_image | image_url: width: top_center_image_width }} 1x, {{ top_center_image | image_url: width: top_center_image_width_2x }} 2x"
          alt="{{ top_center_image.alt | default: '' | escape }}"
          loading="lazy"
          width="{{ top_center_image_width }}"
          height="{{ top_center_image.height | times: top_center_image_width | divided_by: top_center_image.width }}"
        >
      </div>
    {%- endif -%}
    <div class="split-top-content">
      {%- if top_heading != blank -%}
        <h1 class="split-top-heading">{{ top_heading }}</h1>
      {%- endif -%}
      
      {%- if top_subheading != blank -%}
        <p class="split-top-subheading">{{ top_subheading }}</p>
      {%- endif -%}
      
      {%- if top_button_text != blank or top_button_text_2 != blank -%}
        <div class="split-top-buttons split-top-buttons--{{ button_location }}">
          {%- if top_button_text != blank and top_button_url != blank -%}
            <a href="{{ top_button_url }}" class="split-button split-button--primary">{{ top_button_text }}</a>
          {%- endif -%}
          {%- if top_button_text_2 != blank and top_button_url_2 != blank -%}
            <a href="{{ top_button_url_2 }}" class="split-button split-button--secondary">{{ top_button_text_2 }}</a>
          {%- endif -%}
        </div>
      {%- endif -%}
    </div>
  </div>

  <div class="split-bottom">
    <div class="split-bottom-container">
      {%- if feature_blocks.size > 0 -%}
        <div class="split-features-grid split-features-grid--{{ features_layout }}">
          {%- for block in feature_blocks -%}
            {%- liquid
              assign image_position = block.settings.image_position | default: 'top'
              assign title_color = block.settings.title_color
              assign description_color = block.settings.description_color
              assign image_size = block.settings.image_size | default: 120
              assign image_padding = block.settings.image_padding | default: 0
              assign box_width = block.settings.box_width
              assign box_height = block.settings.box_height
              assign animation_type = block.settings.animation_type | default: 'fade-up'
              assign animation_speed = block.settings.animation_speed | default: 800
              assign animation_style = block.settings.animation_style | default: 'ease-out'
              assign animation_delay = block.settings.animation_delay | default: 0
              assign feature_description_2 = block.settings.feature_description_2
              assign desc1_size = block.settings.desc1_size | default: 1.5
              assign desc2_size = block.settings.desc2_size | default: 1.5
              assign desc1_bold = block.settings.desc1_bold | default: false
              assign desc2_bold = block.settings.desc2_bold | default: false
              assign desc1_position = block.settings.desc1_position | default: 'left'
              assign desc2_position = block.settings.desc2_position | default: 'right'
              assign desc1_alignment = block.settings.desc1_alignment | default: 'left'
              assign desc2_alignment = block.settings.desc2_alignment | default: 'right'
              assign center_image_position = block.settings.center_image_position | default: 'center'
              assign right_offset_value = block.settings.right_offset_value | default: 20
              assign title_size = block.settings.title_size | default: 2
              assign description_size = block.settings.description_size | default: 1.5
            -%}
            <div 
              class="split-feature-box split-feature-box--image-{{ image_position }} split-feature-box--animate split-feature-box--{{ animation_type }}" 
              data-animation-type="{{ animation_type }}"
              data-animation-speed="{{ animation_speed }}"
              data-animation-style="{{ animation_style }}"
              data-animation-delay="{{ animation_delay }}"
              {{ block.shopify_attributes }}
              style="{% if title_color != blank %}--feature-title-color: {{ title_color }};{% endif %}{% if description_color != blank %}--feature-description-color: {{ description_color }};{% endif %}{% if image_size != blank %}--feature-image-size: {{ image_size }}px;{% endif %}{% if image_padding != blank %}--feature-image-padding: {{ image_padding }}px;{% endif %}{% if box_width != blank and box_width > 0 %}--feature-box-width: {{ box_width }}px;{% endif %}{% if box_height != blank and box_height > 0 %}--feature-box-height: {{ box_height }}px;{% endif %}{% if image_position == 'middle' %}--desc1-size: {{ desc1_size }}rem;--desc2-size: {{ desc2_size }}rem;--desc1-weight: {% if desc1_bold %}700{% else %}400{% endif %};--desc2-weight: {% if desc2_bold %}700{% else %}400{% endif %};--desc1-align: {{ desc1_alignment }};--desc2-align: {{ desc2_alignment }};{% endif %}{% if image_position == 'right-offset' %}--right-offset-value: {{ right_offset_value }}px;{% endif %}--feature-title-size: {{ title_size }}rem;--feature-description-size: {{ description_size }}rem;--animation-duration: {{ animation_speed }}ms;--animation-timing: {{ animation_style }};transition: opacity {{ animation_speed }}ms {{ animation_style }}, transform {{ animation_speed }}ms {{ animation_style }}, box-shadow 0.3s ease;"
            >
              {%- if image_position == 'middle' -%}
                {%- comment -%} Left side content {%- endcomment -%}
                <div class="split-feature-middle-left">
                  {%- if desc1_position == 'left' and block.settings.feature_description != blank -%}
                    <p class="split-feature-description split-feature-description--middle split-feature-description--desc1">{{ block.settings.feature_description }}</p>
                  {%- endif -%}
                  {%- if desc2_position == 'left' and feature_description_2 != blank -%}
                    <p class="split-feature-description split-feature-description--middle split-feature-description--desc2">{{ feature_description_2 }}</p>
                  {%- endif -%}
                </div>
                
                {%- comment -%} Middle image/icon {%- endcomment -%}
                {%- if block.settings.feature_image != blank -%}
                  <div class="split-feature-image split-feature-image--middle split-feature-image--middle-{{ center_image_position }}">
                    <img
                      src="{{ block.settings.feature_image | image_url: width: 400 }}"
                      alt="{{ block.settings.feature_image.alt | default: block.settings.feature_title | escape }}"
                      loading="lazy"
                      width="{{ block.settings.feature_image.width }}"
                      height="{{ block.settings.feature_image.height }}"
                    >
                  </div>
                {%- elsif block.settings.feature_icon != blank -%}
                  <div class="split-feature-icon split-feature-icon--middle split-feature-icon--middle-{{ center_image_position }}">
                    {{ block.settings.feature_icon }}
                  </div>
                {%- endif -%}
                
                {%- comment -%} Right side content {%- endcomment -%}
                <div class="split-feature-middle-right">
                  {%- if desc1_position == 'right' and block.settings.feature_description != blank -%}
                    <p class="split-feature-description split-feature-description--middle split-feature-description--desc1">{{ block.settings.feature_description }}</p>
                  {%- endif -%}
                  {%- if desc2_position == 'right' and feature_description_2 != blank -%}
                    <p class="split-feature-description split-feature-description--middle split-feature-description--desc2">{{ feature_description_2 }}</p>
                  {%- endif -%}
                </div>
              {%- elsif image_position == 'left-edge' or image_position == 'right-edge' -%}
                {%- if block.settings.feature_image != blank -%}
                  <div class="split-feature-image split-feature-image--{{ image_position }}">
                    <img
                      src="{{ block.settings.feature_image | image_url: width: 400 }}"
                      alt="{{ block.settings.feature_image.alt | default: block.settings.feature_title | escape }}"
                      loading="lazy"
                      width="{{ block.settings.feature_image.width }}"
                      height="{{ block.settings.feature_image.height }}"
                    >
                  </div>
                {%- elsif block.settings.feature_icon != blank -%}
                  <div class="split-feature-icon split-feature-icon--{{ image_position }}">
                    {{ block.settings.feature_icon }}
                  </div>
                {%- endif -%}
                
                {%- if block.settings.feature_title != blank -%}
                  <h3 class="split-feature-title">{{ block.settings.feature_title }}</h3>
                {%- endif -%}
                
                {%- if block.settings.feature_description != blank -%}
                  <p class="split-feature-description">{{ block.settings.feature_description }}</p>
                {%- endif -%}
              {%- else -%}
                {%- if block.settings.feature_image != blank -%}
                  <div class="split-feature-image">
                    <img
                      src="{{ block.settings.feature_image | image_url: width: 400 }}"
                      alt="{{ block.settings.feature_image.alt | default: block.settings.feature_title | escape }}"
                      loading="lazy"
                      width="{{ block.settings.feature_image.width }}"
                      height="{{ block.settings.feature_image.height }}"
                    >
                  </div>
                {%- elsif block.settings.feature_icon != blank -%}
                  <div class="split-feature-icon">
                    {{ block.settings.feature_icon }}
                  </div>
                {%- endif -%}
                
                {%- if block.settings.feature_title != blank -%}
                  <h3 class="split-feature-title">{{ block.settings.feature_title }}</h3>
                {%- endif -%}
                
                {%- if block.settings.feature_description != blank -%}
                  <p class="split-feature-description">{{ block.settings.feature_description }}</p>
                {%- endif -%}
              {%- endif -%}
            </div>
          {%- endfor -%}
        </div>
      {%- endif -%}
    </div>
  </div>
</section>

<style>
  /* Split Section Base */
  .split-section--{{ section_id }} {
    width: 100%;
    max-width: var(--split-section-width, var(--page-width, 120rem));
    margin: 0 auto;
    border-radius: var(--split-section-radius);
    overflow-x: hidden;
    overflow-y: visible;
    background: var(--split-bottom-bg, rgba(var(--color-background), 1));
    box-shadow: 0 4px 24px rgba(var(--color-foreground), 0.08);
  }

  /* Top Area (Hero) */
  .split-section--{{ section_id }} .split-top {
    width: 100%;
    min-height: {{ top_min_height }}vh;
    display: flex;
    align-items: center;
    justify-content: center;
    position: relative;
    padding: 4rem 2rem;
    background: var(--split-top-bg, rgba(var(--color-foreground), 0.03));
    background-image: var(--split-top-bg-image, none);
    background-size: cover;
    background-position: center;
    background-repeat: no-repeat;
    border-radius: var(--split-section-radius) var(--split-section-radius) 0 0;
    overflow: visible;
  }

  /* Maintain 1600x684px aspect ratio when background image is present */
  .split-section--{{ section_id }} .split-top[style*="background-image"] {
    aspect-ratio: 1600 / 684;
    min-height: auto;
  }

  /* Override for gradient background */
  .split-section--{{ section_id }} .split-top[style*="background:"] {
    background-image: none !important;
  }

  .split-section--{{ section_id }} .split-top::before {
    content: '';
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: rgba(var(--color-foreground), 0.1);
    z-index: 1;
  }

  /* Hide overlay when gradient is used */
  .split-section--{{ section_id }} .split-top--gradient::before {
    display: none;
  }

  /* Top Logo */
  .split-section--{{ section_id }} .split-top-logo {
    position: absolute;
    top: 2rem;
    right: 2rem;
    z-index: 3;
    width: var(--top-logo-width, 120px);
    max-width: 200px;
    height: auto;
  }

  .split-section--{{ section_id }} .split-top-logo img {
    width: 100%;
    height: auto;
    display: block;
    object-fit: contain;
  }

  /* Top center image (over background) */
  .split-section--{{ section_id }} .split-top-center-image {
    position: absolute;
    z-index: 2;
    width: var(--top-center-image-width, 280px);
    max-width: 90%;
    pointer-events: none;
  }

  /* Top positions */
  .split-section--{{ section_id }} .split-top-center-image--top-left {
    top: 2rem;
    left: 2rem;
    transform: translateY(var(--top-center-image-y-offset, 0px));
  }

  .split-section--{{ section_id }} .split-top-center-image--top-center {
    top: 2rem;
    left: 50%;
    transform: translate(-50%, var(--top-center-image-y-offset, 0px));
  }

  .split-section--{{ section_id }} .split-top-center-image--top-right {
    top: 2rem;
    right: 2rem;
    left: auto;
    transform: translateY(var(--top-center-image-y-offset, 0px));
  }

  /* Center positions */
  .split-section--{{ section_id }} .split-top-center-image--center-left {
    top: 50%;
    left: 2rem;
    transform: translate(0, calc(-50% + var(--top-center-image-y-offset, 0px)));
  }

  .split-section--{{ section_id }} .split-top-center-image--center {
    top: 50%;
    left: 50%;
    transform: translate(-50%, calc(-50% + var(--top-center-image-y-offset, 0px)));
  }

  .split-section--{{ section_id }} .split-top-center-image--center-right {
    top: 50%;
    right: 2rem;
    left: auto;
    transform: translate(0, calc(-50% + var(--top-center-image-y-offset, 0px)));
  }

  /* Bottom positions - at the bottom edge with 90px overflow */
  .split-section--{{ section_id }} .split-top-center-image--bottom-left {
    bottom: 0;
    top: auto;
    left: 0;
    transform: translateY(calc(90px + var(--top-center-image-y-offset, 0px)));
  }

  .split-section--{{ section_id }} .split-top-center-image--bottom-center {
    bottom: 0;
    top: auto;
    left: 50%;
    transform: translate(-50%, calc(90px + var(--top-center-image-y-offset, 0px)));
  }

  .split-section--{{ section_id }} .split-top-center-image--bottom-right {
    bottom: 0;
    top: auto;
    right: 0;
    left: auto;
    transform: translateY(calc(90px + var(--top-center-image-y-offset, 0px)));
  }

  .split-section--{{ section_id }} .split-top-center-image img {
    width: 100%;
    height: auto;
    display: block;
    object-fit: contain;
  }

  /* Mobile adjustments for top center image */
  @media screen and (max-width: 768px) {
    .split-section--{{ section_id }} .split-top-center-image {
      max-width: 80%;
    }

    .split-section--{{ section_id }} .split-top-center-image--top-left,
    .split-section--{{ section_id }} .split-top-center-image--bottom-left,
    .split-section--{{ section_id }} .split-top-center-image--center-left {
      left: 1rem;
    }

    .split-section--{{ section_id }} .split-top-center-image--top-right,
    .split-section--{{ section_id }} .split-top-center-image--bottom-right,
    .split-section--{{ section_id }} .split-top-center-image--center-right {
      right: 1rem;
    }

    .split-section--{{ section_id }} .split-top-center-image--top-left,
    .split-section--{{ section_id }} .split-top-center-image--top-center,
    .split-section--{{ section_id }} .split-top-center-image--top-right {
      top: 1rem;
    }

    .split-section--{{ section_id }} .split-top-center-image--bottom-left,
    .split-section--{{ section_id }} .split-top-center-image--bottom-right {
      bottom: 0;
      transform: translateY(90px);
    }

    .split-section--{{ section_id }} .split-top-center-image--bottom-center {
      bottom: 0;
      transform: translate(-50%, 90px);
    }

    .split-section--{{ section_id }} .split-top-center-image--bottom-center {
      transform: translate(-50%, 50%);
    }
  }

  .split-section--{{ section_id }} .split-top-content {
    position: relative;
    z-index: 2;
    text-align: center;
    max-width: 800px;
    width: 100%;
  }

  .split-section--{{ section_id }} .split-top-heading {
    font-size: clamp(2.4rem, 5vw, var(--top-heading-size, 4.8rem));
    font-weight: 700;
    line-height: 1.2;
    margin: 0 0 1.6rem;
    color: rgba(var(--color-foreground), 1);
  }

  .split-section--{{ section_id }} .split-top-subheading {
    font-size: clamp(1.6rem, 2.5vw, 2rem);
    line-height: 1.6;
    margin: 0 0 3.2rem;
    color: rgba(var(--color-foreground), 0.9);
  }

  .split-section--{{ section_id }} .split-top-buttons {
    display: flex;
    gap: 1.6rem;
    flex-wrap: wrap;
  }

  /* Button Location Styles */
  .split-section--{{ section_id }} .split-top-buttons--center {
    justify-content: center;
  }

  .split-section--{{ section_id }} .split-top-buttons--left {
    justify-content: flex-start;
  }

  .split-section--{{ section_id }} .split-top-buttons--right {
    justify-content: flex-end;
  }

  .split-section--{{ section_id }} .split-button {
    display: inline-block;
    padding: 1.4rem 3.2rem;
    border-radius: var(--button-radius, 0.8rem);
    font-size: 1.6rem;
    font-weight: 600;
    text-decoration: none;
    transition: transform 0.2s ease, box-shadow 0.2s ease, background-color 0.2s ease, border-color 0.2s ease;
    border: 2px solid transparent;
  }

  .split-section--{{ section_id }} .split-button--primary {
    background: var(--button-primary-bg, rgba(var(--color-button), 1));
    color: var(--button-primary-text, rgba(var(--color-button-text), 1));
    border-color: var(--button-primary-border, var(--button-primary-bg, rgba(var(--color-button), 1)));
  }

  .split-section--{{ section_id }} .split-button--primary:hover {
    transform: translateY(-2px);
    box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
    opacity: 0.9;
  }

  .split-section--{{ section_id }} .split-button--secondary {
    background: var(--button-secondary-bg, transparent);
    color: var(--button-secondary-text, rgba(var(--color-foreground), 1));
    border-color: var(--button-secondary-border, rgba(var(--color-foreground), 0.3));
  }

  .split-section--{{ section_id }} .split-button--secondary:hover {
    background: var(--button-secondary-bg, rgba(var(--color-foreground), 0.05));
    transform: translateY(-2px);
    opacity: 0.9;
  }

  /* Bottom Area */
  .split-section--{{ section_id }} .split-bottom {
    width: 100%;
    background: var(--split-bottom-bg, rgba(var(--color-background), 1));
    border-radius: 0 0 var(--split-section-radius) var(--split-section-radius);
    padding: var(--split-bottom-padding, 4rem) 2rem;
    overflow-x: visible;
    overflow-y: hidden;
  }

  .split-section--{{ section_id }} .split-bottom-container {
    max-width: 120rem;
    margin: 0 auto;
    overflow-x: visible;
    overflow-y: visible;
  }

  /* Features Grid */
  .split-section--{{ section_id }} .split-features-grid {
    display: grid;
    grid-template-columns: 1fr;
    gap: 2.4rem;
    width: 100%;
    max-width: var(--features-container-width, 100%);
    margin: 0 auto;
    overflow: visible;
  }

  @media screen and (min-width: 750px) {
    /* 2 Columns Layout (for 2 boxes) */
    .split-section--{{ section_id }} .split-features-grid--2-columns {
      grid-template-columns: repeat(2, 1fr);
    }

    /* 3 Columns Layout (for 3+ boxes) */
    .split-section--{{ section_id }} .split-features-grid--3-columns {
      grid-template-columns: repeat(3, 1fr);
    }

    /* 4 Columns Layout (for 4+ boxes) */
    .split-section--{{ section_id }} .split-features-grid--4-columns {
      grid-template-columns: repeat(4, 1fr);
    }

